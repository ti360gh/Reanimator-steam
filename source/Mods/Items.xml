<file id="items.txt.cooked">
  <modify>
    <!-- start remove subscriber and multiplayer flags -->
    <entity id="*">
      <attribute id="bitmask2">
        <bitwise id="subscriberOnly">
          <switch>true</switch>
        </bitwise>
      </attribute>
    </entity>
    <!-- end allow rejuvenation injectors to drop -->
    <!-- start retrainer fixes -->
    <entity id="respec_skills">
      <attribute id="quality">
        <replace>-1</replace>
      </attribute>
    </entity>
    <entity id="respec_attrib">
      <attribute id="quality">
        <replace>-1</replace>
      </attribute>
    </entity>
    <!-- end retrainer fixes-->
  </modify>
</file>